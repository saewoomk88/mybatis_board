<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board.mybatis_board.dao.NoticeDao">

    <insert id="insert1" parameterType="noticeDto">
        insert into notice values(#{num},#{writer},#{title},#{content},#{hit})
    </insert>

    <select id="select" parameterType="java.lang.Integer" resultType="board.mybatis_board.dto.NoticeDto">
        select * from notice where num=#{num}
    </select>

</mapper>